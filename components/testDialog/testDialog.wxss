/* components/testDialog/testDialog.wxss */
.modal{width:100%;height:100%;background:#000;opacity:.6;position:fixed;top:0;left:0;z-index:1;}
.dialog{width:540rpx;position:fixed;left:50%;top:20%;margin-left:-270rpx;border:1px solid red;z-index:2;color:aquamarine;word-wrap:break-word;}
.dialog-header{display:-webkit-flex;}
.dialog-title{flex:4;}
.dialog-close{flex:1;display:-webkit-flex;justify-content:flex-end;}
.dialog-footer{display:-webkit-flex;justify-content:space-around;}
.ok,.cancel{width:180rpx;height:70rpx;line-height:70rpx;}